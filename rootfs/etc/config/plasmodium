config mqtt 'mqtt'
        option host 'dev.mqtt-devices.homewifi.fon.com'
        option port '8883'
        option keepalive '120'
        option first_min_interval '0'
        option first_max_interval '10'
        option reconnect_min_interval '120'
        option reconnect_max_interval '9720'
        option reconnect_multiplier '3000'
        option reconnect_planned_min '0'
        option reconnect_planned_max '0'
        option cafile '/etc/ssl/certs/ca-device.crt'
        option capath '/etc/ssl/certs'
        option certfile '/etc/ssl/certs/certificate-device.pem'
        option keyfile '/etc/ssl/certs/private-device-key.pem.key'
        option prefix 'devices'
        option group 'all'
        option requests_prefix 'request'
        option responses_prefix 'response'
        option notifications_prefix 'notification'
        option online_object 'device'
        option online_event 'status'
        option online_retained '0'
        option mode 'username'
        option tls_enabled '1'
        option request_qos '0'
        option response_qos '0'
        option notification_qos '1'
        option version '3'

config services 'services'
        list service 'wifimon'
        list service 'nemo'
        list service 'dhcp'
        list service 'dnsmasq'
        list service 'bandwidth'
        list service 'ookla'
        list service 'network.status:bridge.mac.add'
        list service 'network.status:bridge.mac.del'
        list service 'apcloud_event'
        list service 'apcloud'
        list service 'fonendoscope'
        list service 'mesh:network'
        list service 'mesh:history'
        list service 'log'
        list service 'stats:wan.event'	
	list service 'stats:iptv.wan.event'
        list service 'station_stats:usage'
        list service 'rpcnwi:nwihistory'

config system 'system'
        option board_retries '5'
        option board_tout '5'
        option firstboot '1'

config user 'user'
	option interval '1'

